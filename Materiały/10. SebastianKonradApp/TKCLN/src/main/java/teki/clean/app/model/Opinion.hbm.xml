<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="teki.clean.app.model">
	<class name="Opinion" table="OPINIONS">
		<id name="opinion_id" column="OPINION_ID">
			<generator class="native"/>
		</id>
		<property name="offer_id" column="OFFER_ID" />
		<property name="cleaner_id" column="CLEANER_ID" />
		<property name="text" column="TEXT" />
		<property name="rate" column="RATE" />
	</class>	
</hibernate-mapping>
